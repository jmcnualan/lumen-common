{
    "name": "dmn/cmn",
    "description": "For lumen",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "martin"
        }
    ],
    "require": {
        "dmn/exception": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dmn\\Cmn\\": "/src"
        },
        "files": [
            "helpers/general.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\": "tests/",
            "Dmn\\Cmn\\": "src/"
        }
    },
    "require-dev": {
        "tymon/jwt-auth": "1.0.x-dev",
        "orchestra/testbench": "^6.19",
        "elasticsearch/elasticsearch": "^7.11",
        "laravel/lumen-framework": "^8"
    }
}